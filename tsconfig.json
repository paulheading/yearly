{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "#*": ["./src/*"],
      "#components/*": ["./src/components/*.astro"],
      "#creators/*": ["./src/scripts/creators/*.js"],
      "#commands/*": ["./cypress/support/commands/*.js"],
      "#cypress/*": ["./cypress/*"],
      "#data/*": ["./src/data/*.js"],
      "#display/*": ["./src/scripts/display/*.js"],
      "#e2e/*": ["./cypress/support/e2e/*.js"],
      "#filters": ["./src/scripts/filters.js"],
      "#filters/*": ["./src/scripts/filters/*.js"],
      "#functions": ["./src/styles/_functions.scss"],
      "#functions/*": ["./src/styles/functions/*.scss"],
      "#getters/*": ["./src/scripts/getters/*.js"],
      "#global": ["./src/styles/global.scss"],
      "#helpers": ["./src/scripts/helpers.js"],
      "#helpers/*": ["./src/scripts/helpers/*.js"],
      "#layouts/*": ["./src/layouts/*.astro"],
      "#listeners/*": ["./src/scripts/listeners/*.js"],
      "#loaders/*": ["./src/scripts/loaders/*.js"],
      "#markdown/*": ["./src/markdown/*.md"],
      "#mixins": ["./src/styles/_mixins.scss"],
      "#mixins/*": ["./src/styles/mixins/*.scss"],
      "#pages/*": ["./src/scripts/pages/*.js"],
      "#posters/*": ["./src/scripts/posters/*.js"],
      "#printers/*": ["./src/scripts/printers/*.js"],
      "#putters/*": ["./src/scripts/putters/*.js"],
      "#scripts/*": ["./src/scripts/*.js"],
      "#sections/*": ["./src/components/Sections/*.astro"],
      "#selectors": ["./src/scripts/selectors.js"],
      "#selectors/*": ["./src/scripts/selectors/*.js"],
      "#setters/*": ["./src/scripts/setters/*.js"],
      "#sorters": ["./src/scripts/sorters.js"],
      "#sorters/*": ["./src/scripts/sorters/*.js"],
      "#styles/*": ["./src/styles/*.scss"],
      "#using/*": ["./src/scripts/using/*.js"],
      "#vars": ["./src/styles/_vars.scss"],
      "#vars/*": ["./src/styles/vars/*.scss"]
    }
  },
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}
