---
import Layout from "#layouts/Main";
import { Wrap } from "paully";
import { Content } from "#markdown/userguide.md";
import Markdown from "#components/Markdown.astro";
import { seo } from "paully/data";

const layoutProps = {
  title: "Home | Yearly | Songs you discovered this year",
  keywords: seo.keywords("yearly, spotify, songs, discover"),
  description: "I'm a " + seo.job.title + ". " + seo.job.description,
};

let props = {
  Content,
};
---

<Layout {...layoutProps}>
  <Wrap>
    <Markdown {...props} />
  </Wrap>
</Layout>
