---
import { Wrap } from "paully";
import Section from "~components/Section";
import BackButton from "~components/Buttons/Back";
import { section } from "~scripts/selectors/data";

let sectionProps = {
  name: section.empty_playlist,
  style: {
    display: "none",
  },
};
---

<Section {...sectionProps}>
  <p>sorry mate! you got no songs?!</p>
  <Wrap style={{ marginTop: "2rem" }}>
    <BackButton>Back to start</BackButton>
  </Wrap>
</Section>
