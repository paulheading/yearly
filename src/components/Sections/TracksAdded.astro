---
import Section from "~Components/Section";

let name = "tracks-added";

let sectionProps = {
  name,
  style: {
    display: "none",
  },
};
---

<Section {...sectionProps} className={name}>
  <p>
    <strong data-print={name}>0</strong> tracks added since
    <span data-print="year-added">2025</span>
  </p>
</Section>

<style lang="scss">
  .tracks-added {
    margin-top: 3rem;
  }
</style>
