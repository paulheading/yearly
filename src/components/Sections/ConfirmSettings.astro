---
import Section from "~components/Section";
import data from "~scripts/selectors/data";

let sectionProps = {
  name: data.section.confirm_settings,
  style: {
    marginBottom: "3rem",
    display: "none",
  },
};
---

<Section {...sectionProps}>
  <div class="box"></div>
</Section>

<style lang="scss">
  @use "~mixins" as *;

  .box {
    @include box;
  }

  .setting {
    @include setting;
  }
</style>
