---
import SelectForm from "~components/SelectForm";

let title = "Liked songs";
let value = 0;

let data = {
  state: "closed",
  snake: "choose_source",
  type: "select",
  setting: true,
  group: null,
  card: null,
};

let props = {
  options: [{ title, data: value }],
  label: "Source",
  title,
  value,
  data,
};
---

<SelectForm {...props} className="o" />

<style lang="scss">
  @use "~mixins" as *;

  .o {
    margin-top: 2rem;
  }
</style>
