---
import SelectList from "~Components/SelectList";

let title = "Liked songs";
let value = 0;

let props = {
  label: "Source",
  title,
  value,
  options: [{ title, data: value }],
};
---

<SelectList {...props} className="choose-source" />

<style lang="scss">
  @use "~mixins" as *;

  .choose-source {
    margin-top: 2rem;
  }
</style>
