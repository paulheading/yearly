---
import SelectList from "~components/SelectList";

let snake = "choose_source";
let title = "Liked songs";
let value = 0;

let props = {
  snake,
  label: "Source",
  options: [{ title, data: value }],
  title,
  value,
  type: "select",
};
---

<SelectList {...props} className="o" />

<style lang="scss">
  @use "~mixins" as *;

  .o {
    margin-top: 2rem;
  }
</style>
