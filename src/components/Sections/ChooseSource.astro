---
import SelectForm from "~components/SelectForm";
import $cy from "~scripts/selectors/$cy";

let title = "Liked songs";
let value = 0;

let { choose_source } = $cy.selectForm;

let data = {
  snake: choose_source,
  state: "closed",
  type: "select",
  setting: true,
  group: null,
  card: null,
};

let props = {
  options: [{ title, data: value }],
  label: "Source",
  title,
  value,
  data,
};
---

<SelectForm {...props} className="o" />

<style lang="scss">
  @use "~mixins" as *;

  .o {
    margin-top: 2rem;
  }
</style>
