---
import SelectForm from "~components/SelectForm";

let title = "Liked songs";
let value = 0;

let props = {
  options: [{ title, data: value }],
  snake: "choose_source",
  label: "Source",
  type: "select",
  title,
  value,
};
---

<SelectForm {...props} className="o" />

<style lang="scss">
  @use "~mixins" as *;

  .o {
    margin-top: 2rem;
  }
</style>
