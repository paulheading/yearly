---
import SelectList from "~Components/SelectList";

let data = "choose_source";
let title = "Liked songs";
let value = 0;

let props = {
  data,
  label: "Source",
  options: [{ title, data: value }],
  title,
  value,
};
---

<SelectList {...props} className={data} />

<style lang="scss">
  @use "~mixins" as *;

  .choose_source {
    margin-top: 2rem;
  }
</style>
